{
  "name": "woostrap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass theme/sass/ -o ./theme --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "build": "webpack",
    "dev": "wp-scripts start",
    "compile:css": "node-sass theme/sass/ -o ./theme && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style theme/assets/sass/**/*.scss",
    "lint:scss:fix": "wp-scripts lint-style theme/assets/sass/**/*.scss --fix",
    "lint:js": "wp-scripts lint-js theme/assets/js/**/*.js scripts/**/*.js",
    "install:composer": "npm run wp-env run composer install",
    "install:woocommerce": "wp-env run cli wp plugin install woocommerce --activate --force",
    "install:plugins": "npm run install:woocommerce",
    "lint:php": "npm run lint:php:wpcs && npm run lint:php:parallel",
    "lint:php:wpcs": "npm run wp-env run composer run-script lint:wpcs",
    "lint:php:wpcs:fix": "npm run wp-env run composer run-script lint:wpcs:fix",
    "lint:php:parallel": "npm run wp-env run composer run-script lint:php",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map",
    "setup:data": "npx wp-env run cli bash wp-content/plugins/test-data/env/script.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-e2e": "jest --config e2e.config.js",
    "test-e2e:headless": "cross-env HEADLESS=true npm run test-e2e",
    "test-e2e:reset": "npx wp-env run tests-cli \"wp db reset --yes\"",
    "test-e2e:setup": "npx wp-env run tests-cli bash wp-content/plugins/test-data/tests/script.sh",
    "test-e2e:install": "wp-env run tests-cli \"wp plugin install woocommerce --activate --force\"",
    "wp-env": "npx wp-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sainthkh/woostrap.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sainthkh/woostrap/issues"
  },
  "homepage": "https://github.com/sainthkh/woostrap#readme",
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "dependencies": {
    "@woostrap/playwright-utils": "file:packages/playwright-utils",
    "@woostrap/test-data": "file:packages/test-data",
    "bootstrap": "4.5.2",
    "jquery": "3.5.1",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "@types/jest": "26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@wordpress/env": "1.6.0",
    "@wordpress/eslint-plugin": "7.1.0",
    "@wordpress/prettier-config": "0.3.0",
    "@wordpress/scripts": "12.1.1",
    "cross-env": "7.0.2",
    "dir-archiver": "^1.1.1",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "expect-playwright": "0.2.5",
    "file-loader": "6.0.0",
    "fs-extra": "9.0.1",
    "lerna": "3.22.1",
    "jest": "26.2.2",
    "jest-circus": "26.2.2",
    "jest-playwright-preset": "1.3.0",
    "modularscale-sass": "3.0.10",
    "node-sass": "^4.14.0",
    "playwright": "1.3.0",
    "prettier": "npm:wp-prettier@2.0.5",
    "rtlcss": "^2.5.0",
    "ts-jest": "26.2.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "yargs": "15.4.1"
  }
}
